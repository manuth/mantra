{
    "name": "manuth/test-website",
    "version": "1.0.0",
    "type": "project",
    "description": "A website for testing the `mantra`-theme",
    "authors": [
        {
            "name": "Manuel Thalmann",
            "email": "m@nuth.ch"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.3.3@stable"
    },
    "extra": {
        "expose": [
            "themes/mantra/javascript",
            "themes/mantra/css"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    }
}
